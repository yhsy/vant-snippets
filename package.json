{
  "name": "vant-snippets",
  "displayName": "Vant Snippets",
  "version": "0.1.6",
  "publisher": "fishku",
  "author": {
    "name": "fishku"
  },
  "categories": [
    "Snippets"
  ],
  "description": "Vant Snippets for VS Code",
  "icon": "images/logo.png",
  "keywords": [
    "vant",
    "mobile",
    "vue",
    "snippets"
  ],
  "galleryBanner": {
    "color": "#549BFF",
    "theme": "dark"
  },
  "activationEvents": [
    "onLanguage:vuejs"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "main": "./out/extension",
  "devDependencies": {
    "vscode": "0.10.x"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yhsy/vant-snippets/issues",
    "email": "juerui@vip.qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yhsy/vant-snippets"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/vant-ui.json"
      },
      {
        "language": "html",
        "path": "./snippets/vant-ui.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vant-ui.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/vant-ui.json"
      }
    ]
  }
}